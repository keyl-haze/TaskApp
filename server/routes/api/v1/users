const express = require('express');
const router = new express.Router();
const controller = require(`${__serverRoot}/api/v1/user/controller`);
const { promiseController } = require(`${__serverRoot}/api/v1/utils`);
console.log('controller', controller);
router.get('/', promiseController(controller.list));
router.get('/:id', promiseController(controller.get));
router.post('/', promiseController(controller.create));
module.exports = router;
